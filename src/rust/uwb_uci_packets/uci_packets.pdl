    VENDOR_RESERVED_F = 0x0f,
}

enum DataPacketFormat: 4 {
    DATA_SND = 0x01,
    DATA_RCV = 0x02,
    RADAR_DATA_MESSAGE = 0x0f,
}

// Define a merged enum across GroupId & DataPacketFormat as they are at the same bits in
// |UciPacketHal|.
